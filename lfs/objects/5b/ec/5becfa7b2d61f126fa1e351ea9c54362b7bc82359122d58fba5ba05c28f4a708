using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class EmptyState : State
{
    public EmptyState(FiniteStateMachine machine) : base(machine) { }
    public override void Enter()
    {

    }
    public override void LogicUpdate()
    {
        Debug.Log("EmptyState LogicUpdate, Make sure the state machine is initialized");
    }
    public override void PhysicsUpdate()
    {
        Debug.Log("EmptyState PhysicsUpdate, Make sure the state machine is initialized");
    }
    public override void Exit()
    {

    }
}
