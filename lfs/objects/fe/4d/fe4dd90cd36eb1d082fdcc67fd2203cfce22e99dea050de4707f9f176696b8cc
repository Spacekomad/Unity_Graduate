using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class HereticIdleState : HereticSuperState
{
    HereticIdleState(Character_Heretic character, FiniteStateMachine machine) : base(character, machine) { }

    private float _timer;

    public override void Enter()
    {
        base.Enter();
        _timer = 0.0f;
    }

    public override void LogicUpdate()
    {
        base.LogicUpdate();
    }

    public override void PhysicsUpdate()
    {
        base.PhysicsUpdate();
    }

    public override void Exit()
    {
        base.Exit();
    }
}
